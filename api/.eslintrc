{
   "env": {
      "node": true,
      "es6": true
   },
   "extends": ["airbnb-base", "plugin:prettier/recommended"],
   "plugins": ["import"],
   "parserOptions": {
      "ecmaVersion": 2023,
      "sourceType": "module"
   },
   "rules": {
      "prettier/prettier": ["error", { "endOfLine": "lf" }],
      "import/extensions": ["error", "ignorePackages"],
      "import/no-unresolved": "error"
   }
}
